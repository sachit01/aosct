    <LocalDebuggerCommandArguments>-n</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)..\..\data_files</LocalDebuggerWorkingDirectory>
