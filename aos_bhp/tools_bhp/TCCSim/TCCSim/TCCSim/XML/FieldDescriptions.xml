    <Detail>Type of supervision</Detail>
    <Length>1</Length>
    <Min>0</Min>
    <Max>255</Max>
    <Resolution></Resolution>
    <Format>UINT</Format>
    <Special>
      <Fields>
        <Field value="0">No supervision</Field>
