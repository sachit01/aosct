	<xs:element name="LogEventTypeInfo">
		<xs:annotation>
			<xs:documentation>A log producer system registers an event type, for subsequent event logs</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.2: Added element "interpretation/list/text"</xs:appinfo>
			<xs:appinfo>V 1.5: New version of "tVariableSpecification", added "supervision" element</xs:appinfo>
			<xs:appinfo>V 1.6: Added element "keyword"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="eventType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the event type</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="description" type="rcs:tDescription">
					<xs:annotation>
						<xs:documentation>Plain language event text</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="severity" type="rcs:tLogEventSeverity">
					<xs:annotation>
						<xs:documentation>Default value, may be overridden by local MDC configuration</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="operatorAck" type="rcs:tEmpty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Indicates if operator acknowledge is required. Default value, may be overridden by local MDC configuration</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="juridical" type="rcs:tEmpty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Indicates if events shall be logged in JRU. Default value, may be overridden by local MDC configuration</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="interpretation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Rules for presenting attached data, when applicable</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="java" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Java class for interpreting binary data</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="class" type="xs:string">
											<xs:annotation>
												<xs:documentation>Fully qualified class name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="jar" type="xs:anyURI" minOccurs="0">
											<xs:annotation>
												<xs:documentation>URL to JAR file location</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="xslt" type="xs:anyURI" minOccurs="0">
								<xs:annotation>
									<xs:documentation>URL to XSLT style sheet for interpreting XML data</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="list" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="item" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Specification of list values to be supplied</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="rcs:tVariableSpecification">
														<xs:sequence>
															<xs:element name="snmp" minOccurs="0">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="oid" type="xs:string">
																			<xs:annotation>
																				<xs:documentation>The SNMP OID of a variable attached to the trap</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="text" type="rcs:tDescription" minOccurs="0">
											<xs:annotation>
												<xs:documentation>A plain language text with placeholders to substitute for actual list values</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
