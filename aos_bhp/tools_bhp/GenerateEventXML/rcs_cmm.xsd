<?xml version="1.0" encoding="utf-8"?>
<!-- edited with XMLSpy v2017 sp2 (x64) (http://www.altova.com) by Martin Karlsson (Bombardier Transportation Sweden AB) -->
<xs:schema xmlns:rcs="http://www.transport.bombardier.com/2014/RcsCmm" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.transport.bombardier.com/2014/RcsCmm" version="1.6" xml:lang="en" vc:minVersion="1.0">
	<!--Layer 1: Message Framework-->
	<!--Root Element-->
	<xs:element name="Message">
		<xs:annotation>
			<xs:documentation>The frame to be used by all messages</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Added "correlationId" element, element "timestamp" added to "hdr" element, allowed multiple instances of "content", removed "privilege", set "refBase" optional</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="hdr">
					<xs:annotation>
						<xs:documentation>Common message header</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="refBase" type="xs:anyURI" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The base for relative resource references in the data part</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:choice>
								<xs:element name="safety" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Safety message header</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="source" type="rcs:tResourceRef">
												<xs:annotation>
													<xs:documentation>The system component producing the message</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="destination" type="rcs:tResourceRef">
												<xs:annotation>
													<xs:documentation>The receiving system component or message queue</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="seqNo" type="xs:unsignedLong">
												<xs:annotation>
													<xs:documentation>Sequential number of message on this source/destination channel</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="timestamp" type="xs:dateTime">
												<xs:annotation>
													<xs:documentation>Message creation time, in ms resolution</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="hashType">
												<xs:annotation>
													<xs:documentation>Algorithm used for hash string calculation</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:union>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="MD5"/>
																<xs:enumeration value="SHA-256"/>
															</xs:restriction>
														</xs:simpleType>
														<xs:simpleType>
															<xs:restriction base="xs:string"/>
														</xs:simpleType>
													</xs:union>
												</xs:simpleType>
											</xs:element>
											<xs:element name="hash" type="xs:string">
												<xs:annotation>
													<xs:documentation>Hash value, calculated over message contents</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:sequence>
									<xs:element name="source" type="rcs:tResourceRef" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The system component producing the message</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="timestamp" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Message creation time, in ms resolution</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:choice>
							<xs:element name="replyTo" type="rcs:tMsqQueueRef" minOccurs="0">
								<xs:annotation>
									<xs:documentation>JMS queue for response message</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="messageId" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Identifier of the message in request-reply pattern.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="correlationId" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Identifier of the request message sent by the replier in the request-reply pattern. Value of "correlationId" is the same as the value of "messageId" received in the request.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="content" type="xs:string" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Fully qualified names of the child elements to "data"</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="data">
					<xs:complexType>
						<xs:sequence>
							<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Extension point for message content elements</xs:documentation>
								</xs:annotation>
							</xs:any>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Simple types-->
	<xs:simpleType name="tEmpty">
		<xs:annotation>
			<xs:documentation>An empty element (for boolean flags)</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tMsqQueueRef">
		<xs:annotation>
			<xs:documentation>Identity of a JMS queue or topic</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="jms\.queue\..*"/>
			<xs:pattern value="jms\.topic\..*"/>
			<xs:pattern value=""/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tResourceRef">
		<xs:annotation>
			<xs:documentation>Unique identity for an object or other entity handled by the system</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI"/>
	</xs:simpleType>
	<!--Complex types-->
	<xs:complexType name="tDescription">
		<xs:annotation>
			<xs:documentation>A textual description tagged with a language code</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="desc" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Repeat for multiple languages</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="lang" type="xs:language" minOccurs="0">
							<xs:annotation>
								<xs:documentation>BCP 47 language code</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="text" type="xs:string">
							<xs:annotation>
								<xs:documentation>The description text</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tDocumentation">
		<xs:annotation>
			<xs:documentation>A document link tagged with a language code</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="doc" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Repeat for multiple languages</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="lang" type="xs:language" minOccurs="0">
							<xs:annotation>
								<xs:documentation>BCP 47 language code</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="link" type="xs:anyURI">
							<xs:annotation>
								<xs:documentation>The document link</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tTimeSpan">
		<xs:annotation>
			<xs:documentation>Timespan type</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Start of time span</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>End of time span</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Content Elements-->
	<xs:element name="AcknowledgeRequest">
		<xs:annotation>
			<xs:documentation>A receiver of a command requests additional confirmation from the sender before executing</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="command">
					<xs:complexType>
						<xs:sequence>
							<xs:any processContents="lax" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Command payload to be confirmed</xs:documentation>
								</xs:annotation>
							</xs:any>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommandConfirmation">
		<xs:annotation>
			<xs:documentation>Positive response to “AcknowledgeRequest”</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="command">
					<xs:complexType>
						<xs:sequence>
							<xs:any processContents="lax" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Command payload to be confirmed</xs:documentation>
								</xs:annotation>
							</xs:any>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GeneralResponse">
		<xs:annotation>
			<xs:documentation>A generic message, for application level response</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Option "nok", reference to response and free properties added</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="state">
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="ok"/>
									<xs:enumeration value="nok"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
				<xs:element name="response" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>References to static or dynamic response indication resources</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="property" type="rcs:tProperty" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Optional named properties</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Heartbeat">
		<xs:annotation>
			<xs:documentation>Heartbeat message for communication supervision on application level</xs:documentation>
			<xs:appinfo>V 1.3: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="interval" type="xs:duration">
					<xs:annotation>
						<xs:documentation>Maximum interval between messages with the same "Message/hdr/source" on this queue or topic</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RefreshRequest">
		<xs:annotation>
			<xs:documentation>Command to trigger data refresh</xs:documentation>
			<xs:appinfo>V 1.6: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="target" type="rcs:tResourceRef"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RefreshStatus">
		<xs:annotation>
			<xs:documentation>Status of refresh request</xs:documentation>
			<xs:appinfo>V 1.6: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="target" type="rcs:tResourceRef"/>
				<xs:element name="state">
					<xs:annotation>
						<xs:documentation>State</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="refreshStarted"/>
									<xs:enumeration value="refreshReady"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ResourceInfoQuery">
		<xs:annotation>
			<xs:documentation>A generic request for static information, directed to any resource</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="resource" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Reference to resource to be queried</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="pattern" type="xs:string" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Pattern to match against resource identities</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ResourceStatusQuery">
		<xs:annotation>
			<xs:documentation>A generic request for dynamic information, directed to any resource</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="resource" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Reference to resource to be queried</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="pattern" type="xs:string" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Pattern to match against resource identities</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Layer 2: Topology-->
	<!--Simple types-->
	<xs:simpleType name="tTrackDirection">
		<xs:annotation>
			<xs:documentation>Direction relative to the direction of a track</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Added "both" option</xs:appinfo>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="normal"/>
					<xs:enumeration value="reverse"/>
					<xs:enumeration value="both"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<!--Complex types-->
	<xs:complexType name="tArea">
		<xs:annotation>
			<xs:documentation>A delimited area in the railway layout</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Removed "id", "ref", "name" elements. Added "extent"</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="object" type="rcs:tResourceRef" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>All objects included in the area</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence>
				<xs:element name="border" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Positions or objects marking the area border</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="position" type="rcs:tPosition">
								<xs:annotation>
									<xs:documentation>Border position, directed towards the area</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:sequence>
								<xs:element name="object" type="rcs:tResourceRef">
									<xs:annotation>
										<xs:documentation>Border object</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="direction" type="rcs:tTrackDirection">
									<xs:annotation>
										<xs:documentation>Direction from object towards the area</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="internalTrack" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Additional tracks, which are completely inside the area</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:element name="extent" type="rcs:tLinearExtent" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="tLinearExtent">
		<xs:annotation>
			<xs:documentation>An arbitrary contiguous segment of railway line</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="start" type="rcs:tPosition">
				<xs:annotation>
					<xs:documentation>Start position, directed along the extent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end" type="rcs:tPosition">
				<xs:annotation>
					<xs:documentation>End position</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="via" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Intermediate tracks, in sequence</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="length" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total length of the extent</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tLocation">
		<xs:annotation>
			<xs:documentation>A position or an area</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="position" type="rcs:tPosition"/>
			<xs:element name="area" type="rcs:tArea"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="tPathConstraints">
		<xs:annotation>
			<xs:documentation>Directions for searching a path or route</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Added "train" element as start position</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="startPos" type="rcs:tPosition">
					<xs:annotation>
						<xs:documentation>Start position for search, optionally directed</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="startObject" type="rcs:tResourceRef">
						<xs:annotation>
							<xs:documentation>Start search at the position of the referenced object</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="direction" type="rcs:tTrackDirection" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optionally specified direction of search</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="train" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Start search at the position of the referenced train</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="endPos" type="rcs:tPosition">
					<xs:annotation>
						<xs:documentation>Target position</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="endObject" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Use position of the referenced object as target</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="via" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Optional list of tracks to include in the path</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tPosition">
		<xs:annotation>
			<xs:documentation>A geographic position in the railway network</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="trackpos" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="track" type="rcs:tResourceRef">
							<xs:annotation>
								<xs:documentation>Reference to the track where the position is</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="pos">
							<xs:annotation>
								<xs:documentation>Distance in meters from start of track</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="dir" type="rcs:tTrackDirection" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Direction vs direction of the track</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="absPos" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Kilometric position</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="km" type="xs:nonNegativeInteger"/>
						<xs:element name="m">
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="geoCoord" minOccurs="0">
				<xs:annotation>
					<xs:documentation>WGS 84 geodetic coordinates</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="x" type="xs:double"/>
						<xs:element name="y" type="xs:double"/>
						<xs:element name="z" type="xs:double" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="objectOffset" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position related to a reference object</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="object" type="rcs:tResourceRef">
							<xs:annotation>
								<xs:documentation>Reference object</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="direction" type="rcs:tTrackDirection">
							<xs:annotation>
								<xs:documentation>Direction from object to position</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="distance" type="xs:decimal">
							<xs:annotation>
								<xs:documentation>Distance in meters </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tProperty">
		<xs:annotation>
			<xs:documentation>A named property</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.2: Added elements "durationValue" and "dateTimeValue"</xs:appinfo>
			<xs:appinfo>V 1.5: Changed to extension of "tVariable", added "format" element</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rcs:tVariable">
				<xs:sequence>
					<xs:element name="description" type="rcs:tDescription" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Descriptive name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="format" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The printf style format specification</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tVariable">
		<xs:annotation>
			<xs:documentation>To be used where the schema needs to support values of different types</xs:documentation>
			<xs:appinfo>V 1.5: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key" type="xs:string">
				<xs:annotation>
					<xs:documentation>Variable key</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>Variable value</xs:documentation>
				</xs:annotation>
				<xs:element name="boolValue" type="xs:boolean"/>
				<xs:element name="intValue" type="xs:integer"/>
				<xs:element name="floatValue" type="xs:float"/>
				<xs:element name="dateTimeValue" type="xs:dateTime"/>
				<xs:element name="durationValue" type="xs:duration"/>
				<xs:element name="stringValue" type="xs:string"/>
				<xs:element name="positionValue" type="rcs:tPosition"/>
				<xs:element name="refValue" type="rcs:tResourceRef"/>
				<xs:element name="value" type="xs:anySimpleType">
					<xs:annotation>
						<xs:documentation>Deprecated element, included for backward compatibility</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tVariableSpecification">
		<xs:annotation>
			<xs:documentation>Description and data type of a key value</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.2: Added element "unit". Added alternatives "duration" and "dateTime" to element "dataType"</xs:appinfo>
			<xs:appinfo>V 1.5: Added element "format", added alternatives "reference", "position" to element "dataType"</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key" type="xs:string">
				<xs:annotation>
					<xs:documentation>The internal name of the variable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="rcs:tDescription">
				<xs:annotation>
					<xs:documentation>Plain language description of the variable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="dataType">
					<xs:annotation>
						<xs:documentation>Type of data, when variable is a simple data type</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="boolean"/>
									<xs:enumeration value="integer"/>
									<xs:enumeration value="float"/>
									<xs:enumeration value="string"/>
									<xs:enumeration value="duration"/>
									<xs:enumeration value="dateTime"/>
									<xs:enumeration value="reference"/>
									<xs:enumeration value="position"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
				<xs:element name="enumValue" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Possible variable values</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="value" type="xs:anySimpleType">
								<xs:annotation>
									<xs:documentation>Logged value</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="description" type="rcs:tDescription">
								<xs:annotation>
									<xs:documentation>Plain language description of the significance of the value</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="unit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Physical unit of variable value, e.g. "mA" or "km"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="format" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The printf style format specification</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Content Elements-->
	<xs:element name="PathQuery">
		<xs:annotation>
			<xs:documentation>A request to locate all possible paths between two positions</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="constraints" type="rcs:tPathConstraints"/>
				<xs:element name="maxLength" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Optional length limit for search</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PathQueryResponse">
		<xs:annotation>
			<xs:documentation>Response to PathQuery</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Updated element documentation</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="path" type="rcs:tLinearExtent" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Layer 3: Common Logic-->
	<!--Simple types-->
	<xs:simpleType name="tCommonCommand">
		<xs:annotation>
			<xs:documentation>Common command</xs:documentation>
			<xs:appinfo>V 1.5: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="inhibit"/>
			<xs:enumeration value="noInhibit"/>
			<xs:enumeration value="block"/>
			<xs:enumeration value="unblock"/>
			<xs:enumeration value="localRelease"/>
			<xs:enumeration value="revokeLocalRelease"/>
			<xs:enumeration value="activate"/>
			<xs:enumeration value="deactivate"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tLogEventSeverity">
		<xs:annotation>
			<xs:documentation>Severity of an event</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="debug"/>
					<xs:enumeration value="info"/>
					<xs:enumeration value="warning"/>
					<xs:enumeration value="error"/>
					<xs:enumeration value="systemFailure"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="tRequestSource">
		<xs:annotation>
			<xs:documentation>The source of a specific resource state</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="centralOperator"/>
					<xs:enumeration value="centralAutomaton"/>
					<xs:enumeration value="controlSystem"/>
					<xs:enumeration value="localOperator"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<!--Complex types-->
	<xs:complexType name="tEquipmentStatus">
		<xs:annotation>
			<xs:documentation>The status of any control equipment</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.1: Added option "noData" to element "state"</xs:appinfo>
			<xs:appinfo>V 1.5: Added option "inhibited" to element "state"</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="state">
				<xs:annotation>
					<xs:documentation>Summary state</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:union>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="offline"/>
								<xs:enumeration value="starting"/>
								<xs:enumeration value="standby"/>
								<xs:enumeration value="operational"/>
								<xs:enumeration value="degraded"/>
								<xs:enumeration value="noData"/>
								<xs:enumeration value="inhibited"/>
							</xs:restriction>
						</xs:simpleType>
						<xs:simpleType>
							<xs:restriction base="xs:string"/>
						</xs:simpleType>
					</xs:union>
				</xs:simpleType>
			</xs:element>
			<xs:element name="alarm" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Active alarms for the unit, if any</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tNetworkElementStatus">
		<xs:annotation>
			<xs:documentation>Operational status of a part of the rail network</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Removed "state" elment, added "locked", "blocked", "manuallyLocked" and "local" elements</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="locked" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Locking state</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="blocked" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Blocking state</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manuallyLocked" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates manual lock</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="local" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates local control</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source" type="rcs:tRequestSource" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Requestor of current state, if applicable</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alarm" type="rcs:tResourceRef" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Active alarms for the resource, if any</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tSection">
		<xs:annotation>
			<xs:documentation>Section of railway line with optional authorisation information</xs:documentation>
			<xs:appinfo>V 1.5: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="extent" type="rcs:tLinearExtent">
				<xs:annotation>
					<xs:documentation>Section extent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorisationType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of authorisation</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tValidity">
		<xs:annotation>
			<xs:documentation>Validity time information for status data</xs:documentation>
			<xs:appinfo>V 1.0: Type introduced</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="changed" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Time when the status was latest changed, if known</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="confirmed" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Time when the status was latest confirmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validUntil" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An optional limit after which the status should be considered uncertain</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Content Elements-->
	<xs:element name="ConfigurationRequest">
		<xs:annotation>
			<xs:documentation>A component requests configuration file(s) from a central configuration service</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Component ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="version" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Version of component configuration</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="configs" type="xs:string">
					<xs:annotation>
						<xs:documentation>Path to requesting configs: * - for all available configs; test2.xml - for specific file; /path/to/dir/ - for all configs from path; test.xml,path/to/test2.xml – for specific files or hole paths</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConfigurationResponse">
		<xs:annotation>
			<xs:documentation>List of configuration file contents for a component</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Component ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="configs">
					<xs:annotation>
						<xs:documentation>List of configurations</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="config" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="path" type="xs:string"/>
										<xs:element name="fileName" type="xs:string"/>
										<xs:element name="file" type="xs:base64Binary">
											<xs:annotation>
												<xs:documentation>File content in binary form</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="mimeType" type="xs:string">
											<xs:annotation>
												<xs:documentation>File format specification (IANA media type)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConfigurationStatus">
		<xs:annotation>
			<xs:documentation>Notify components about a change in state of a configuration</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Component ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="version" type="xs:string">
					<xs:annotation>
						<xs:documentation>Version of configuration for component</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="state">
					<xs:annotation>
						<xs:documentation>Describes the last change which related to component configuration</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="added"/>
									<xs:enumeration value="updated"/>
									<xs:enumeration value="removed"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ControllerCommand">
		<xs:annotation>
			<xs:documentation>Commands to control the status of connected equipment</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Element "command" extended with a restriction of "tCommonCommand"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="controller" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the controller unit</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="command">
					<xs:annotation>
						<xs:documentation>Command to controller</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="start"/>
									<xs:enumeration value="stop"/>
									<xs:enumeration value="restart"/>
									<xs:enumeration value="standby"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="rcs:tCommonCommand">
									<xs:enumeration value="inhibit"/>
									<xs:enumeration value="noInhibit"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ControllerStatus">
		<xs:annotation>
			<xs:documentation>Health indications from connected equipment</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Element "valid" made optional</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="controller" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the controller unit</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="state" type="rcs:tEquipmentStatus"/>
				<xs:element name="valid" type="rcs:tValidity" minOccurs="0">
					<xs:annotation>
						<xs:documentation>State validity information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="property" type="rcs:tProperty" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Optional named properties</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DerailerCommand">
		<xs:annotation>
			<xs:documentation>Commands to operate a derailer</xs:documentation>
			<xs:appinfo>V 1.5: Element introduced</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="derailer" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the derailer</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="command">
					<xs:annotation>
						<xs:documentation>Command to derailer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="on"/>
									<xs:enumeration value="off"/>
									<xs:enumeration value="lock"/>
									<xs:enumeration value="unlock"/>
									<xs:enumeration value="lockOn"/>
									<xs:enumeration value="lockOff"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="rcs:tCommonCommand">
									<xs:enumeration value="localRelease"/>
									<xs:enumeration value="revokeLocalRelease"/>
									<xs:enumeration value="block"/>
									<xs:enumeration value="unblock"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DerailerStatus">
