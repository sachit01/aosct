	<xs:element name="LogEventStatus">
		<xs:annotation>
			<xs:documentation>A log producer system logs an event</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.2: Added 'processContents="lax"' attribute to the "xs:any" element in "xmlData"</xs:appinfo>
			<xs:appinfo>V 1.5: Value of "item" element changed to "tVariable"</xs:appinfo>
			<xs:appinfo>V 1.6: Added element "location"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="event" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Unique identity for this event</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="eventType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the EventType of this event</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="object" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reference to the source of the event</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="time" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Time of occurrance (or logged state change)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="state">
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="on"/>
									<xs:enumeration value="off"/>
									<xs:enumeration value="pulse"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
				<xs:element name="parent" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reference to the parent event, if this is a subevent</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="listData" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Optional attached data</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="item" type="rcs:tVariable" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="xmlData" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Optional attached data in arbitrary XML form</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:any processContents="lax"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="binaryData" type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Optional attached data in raw binary form</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="location" type="rcs:tLocation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Geographic location of log source (for moving objects)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
