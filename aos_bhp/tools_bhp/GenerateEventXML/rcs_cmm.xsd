			<xs:documentation>DEPRECATED - use LogSourceTypeInfo</xs:documentation>
			<xs:appinfo>V 1.1: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.3: Changed enum set in "measurementPoint/supervision/rule"</xs:appinfo>
			<xs:appinfo>V 1.5: Element replaced by "LogSourceTypeInfo"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="systemType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the system type</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="description" type="rcs:tDescription">
					<xs:annotation>
						<xs:documentation>Plain language system description</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="parent" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A category of system types, from which this type inherits additional characteristics</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="measurementPoint" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>The measurement points which exist in this system type</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="rcs:tVariableSpecification">
								<xs:sequence>
									<xs:element name="tolerance" type="xs:anySimpleType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Optional difference threshold value for logging a measurement as a new value. Default value, may be overridden by local MDC configuration</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="supervision" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Rules for alerting operator about unexpected values. List may be overridden or amended by local MDC configuration</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="rule">
													<xs:annotation>
														<xs:documentation>Type of rule (several of the same type may be listed, e.g. to get a "warning" and an "error" threshold)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:union>
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="maxValue"/>
																	<xs:enumeration value="minValue"/>
																	<xs:enumeration value="blockValue"/>
																	<xs:enumeration value="passValues"/>
																	<xs:enumeration value="maxCount"/>
																	<xs:enumeration value="maxInterval"/>
																	<xs:enumeration value="minInterval"/>
																	<xs:enumeration value="blockPattern"/>
																	<xs:enumeration value="passPattern"/>
																</xs:restriction>
															</xs:simpleType>
															<xs:simpleType>
																<xs:restriction base="xs:string"/>
															</xs:simpleType>
														</xs:union>
													</xs:simpleType>
												</xs:element>
												<xs:element name="threshold" type="xs:anySimpleType">
													<xs:annotation>
														<xs:documentation>Value to check rule against</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="eventType" type="rcs:tResourceRef">
													<xs:annotation>
														<xs:documentation>Event to generate when rule is violated</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="documentation" type="rcs:tDocumentation" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Hyperlink to measurement point documentation</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="snmp" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Information required if measurements are to be collected over SNMP</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="oid" type="xs:string">
													<xs:annotation>
														<xs:documentation>The SNMP OID of the variable containing the measurement</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="pollInterval">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="default" type="xs:nonNegativeInteger">
