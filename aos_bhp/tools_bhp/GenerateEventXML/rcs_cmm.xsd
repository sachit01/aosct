																	<xs:documentation>Default interval in ms to poll the variable. 0 means no polling</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="min" type="xs:positiveInteger">
																<xs:annotation>
																	<xs:documentation>Minimum allowed interval in ms to poll the variable</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="eventTypes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of event types that this system type may log. Needed only when using the negotiated method for configuration transfer</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ref" type="rcs:tResourceRef" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Reference to the event type</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="documentation" type="rcs:tDocumentation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Hyperlink to system type documentation</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LogDeviceTypeInfo">
		<xs:annotation>
			<xs:documentation>DEPRECATED - use LogSourceTypeInfo</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.1: Elements "systemType" and "parent" added</xs:appinfo>
			<xs:appinfo>V 1.3: Changed enum set in "measurementPoint/supervision/rule"</xs:appinfo>
			<xs:appinfo>V 1.5: Element replaced by "LogSourceTypeInfo"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="deviceType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the device type</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="description" type="rcs:tDescription">
					<xs:annotation>
						<xs:documentation>Plain language device description</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="systemType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the system type which this device type belongs to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="parent" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A category of device types, from which this type inherits additional characteristics</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="measurementPoint" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>The measurement points which exist in this device type</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="rcs:tVariableSpecification">
								<xs:sequence>
									<xs:element name="tolerance" type="xs:anySimpleType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Optional difference threshold value for logging a measurement as a new value. Default value, may be overridden by local MDC configuration</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="supervision" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Rules for alerting operator about unexpected values. List may be overridden or amended by local MDC configuration</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="rule">
													<xs:annotation>
														<xs:documentation>Type of rule (several of the same type may be listed, e.g. to get a "warning" and an "error" threshold)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:union>
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="maxValue"/>
																	<xs:enumeration value="minValue"/>
																	<xs:enumeration value="blockValue"/>
																	<xs:enumeration value="passValues"/>
																	<xs:enumeration value="maxCount"/>
																	<xs:enumeration value="maxInterval"/>
																	<xs:enumeration value="minInterval"/>
																	<xs:enumeration value="blockPattern"/>
																	<xs:enumeration value="passPattern"/>
																</xs:restriction>
															</xs:simpleType>
															<xs:simpleType>
																<xs:restriction base="xs:string"/>
															</xs:simpleType>
														</xs:union>
													</xs:simpleType>
												</xs:element>
												<xs:element name="threshold" type="xs:anySimpleType">
													<xs:annotation>
														<xs:documentation>Value to check rule against</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="eventType" type="rcs:tResourceRef">
													<xs:annotation>
														<xs:documentation>Event to generate when rule is violated</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="documentation" type="rcs:tDocumentation" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Hyperlink to measurement point documentation</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="snmp" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Information required if measurements are to be collected over SNMP</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="oid" type="xs:string">
													<xs:annotation>
														<xs:documentation>The SNMP OID of the variable containing the measurement</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="pollInterval">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="default" type="xs:nonNegativeInteger">
																<xs:annotation>
																	<xs:documentation>Default interval in ms to poll the variable. 0 means no polling</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="min" type="xs:positiveInteger">
																<xs:annotation>
																	<xs:documentation>Minimum allowed interval in ms to poll the variable</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="eventTypes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of event types that this device type may log. Needed only when using the negotiated method for configuration transfer</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ref" type="rcs:tResourceRef" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Reference to the event type</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="documentation" type="rcs:tDocumentation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Hyperlink to device type documentation</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LogSystemInfo">
		<xs:annotation>
			<xs:documentation>DEPRECATED - use LogSourceInfo</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.1: Elements "systemType" and "parent" added</xs:appinfo>
			<xs:appinfo>V 1.3: Element "identification" added</xs:appinfo>
			<xs:appinfo>V 1.5: Element replaced by "LogSourceInfo", changed "property" element to old "tProperty" content</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="system" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="name" type="xs:string">
					<xs:annotation>
						<xs:documentation>Presentation name for the system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="systemType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the system type of this system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="parent" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>When applicable, reference to the Logsystem to which this subsystem belongs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="facility" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reference to a LogFacility to which the system belongs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="location" type="rcs:tLocation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Geographic information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="identification" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Asset attributes of system</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="componentId" type="xs:string" minOccurs="0"/>
							<xs:element name="hwVersion" type="xs:string" minOccurs="0"/>
							<xs:element name="swVersion" type="xs:string" minOccurs="0"/>
							<xs:element name="serialNo" type="xs:string" minOccurs="0"/>
							<xs:element name="property" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Optional list of named properties</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="id" type="xs:token">
											<xs:annotation>
												<xs:documentation>A shorthand internal name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="description" type="rcs:tDescription">
											<xs:annotation>
												<xs:documentation>Descriptive name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="intValue" type="xs:integer"/>
											<xs:element name="boolValue" type="xs:boolean"/>
											<xs:element name="floatValue" type="xs:float"/>
											<xs:element name="stringValue" type="xs:string"/>
											<xs:element name="durationValue" type="xs:duration"/>
											<xs:element name="dateTimeValue" type="xs:dateTime"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LogDeviceInfo">
		<xs:annotation>
			<xs:documentation>DEPRECATED - use LogSourceInfo</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Element replaced by "LogSourceInfo", changed "property" element to old "tProperty" content</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="device" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the device</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="name" type="xs:string">
					<xs:annotation>
						<xs:documentation>Presentation name for the device</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="identification" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Asset attributes of device</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="componentId" type="xs:string" minOccurs="0"/>
							<xs:element name="hwVersion" type="xs:string" minOccurs="0"/>
							<xs:element name="swVersion" type="xs:string" minOccurs="0"/>
							<xs:element name="serialNo" type="xs:string" minOccurs="0"/>
							<xs:element name="property" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Optional list of named properties</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="id" type="xs:token">
											<xs:annotation>
												<xs:documentation>A shorthand internal name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="description" type="rcs:tDescription">
											<xs:annotation>
												<xs:documentation>Descriptive name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="intValue" type="xs:integer"/>
											<xs:element name="boolValue" type="xs:boolean"/>
											<xs:element name="floatValue" type="xs:float"/>
											<xs:element name="stringValue" type="xs:string"/>
											<xs:element name="durationValue" type="xs:duration"/>
											<xs:element name="dateTimeValue" type="xs:dateTime"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="deviceType" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the DeviceType describing this device</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="parent" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the Logsystem or LogDevice to which this device belongs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="facility" type="rcs:tResourceRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reference to a LogFacility to which the device belongs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="location" type="rcs:tLocation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Geographic location</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="snmp" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information required if measurements and events are to be collected over SNMP</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ip" type="xs:string">
								<xs:annotation>
									<xs:documentation>IP address of the device</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="context" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>SNMP context string, identifying the device among others on the same IP address</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="measurementPoint" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Any measurement points which will be retrieved from other physical units</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="identity" type="rcs:tResourceRef">
											<xs:annotation>
												<xs:documentation>Measurement point identity, corresponding to a measurement point defined in LogDeviceTypeInfo</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="oid" type="xs:string">
											<xs:annotation>
												<xs:documentation>The SNMP OID of the variable containing the measurement</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ip" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>IP address of the device holding the measurement, if different from the IP address of this device</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="context" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SNMP context string, identifying the device among others on the same IP address</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="eventType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Any traps related to this device, which will be sent by another device</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ref" type="rcs:tResourceRef">
											<xs:annotation>
												<xs:documentation>Reference to the event type</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="oid" type="xs:string">
											<xs:annotation>
												<xs:documentation>The SNMP OID of the trap</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ip" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>IP address of the device which will send the trap, if different from the IP address of this device</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="context" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>SNMP context string, identifying the device among others on the same IP address</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OcpServiceCommand">
		<xs:annotation>
			<xs:documentation>DEPRECATED - use OperationalPointCommand</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Replaced by "OperationalPointCommand"</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ocp" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the OCP</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="command">
					<xs:annotation>
						<xs:documentation>Requested state</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="normal"/>
									<xs:enumeration value="outOfService"/>
									<xs:enumeration value="holdTrain"/>
									<xs:enumeration value="runThru"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:union>
					</xs:simpleType>
				</xs:element>
				<xs:element name="time" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Time when new state is desired</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="source" type="rcs:tRequestSource" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Source of request</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OcpServiceStatus">
		<xs:annotation>
			<xs:documentation>DEPRECATED- use OperationalPointStatus</xs:documentation>
			<xs:appinfo>V 1.0: Element introduced</xs:appinfo>
			<xs:appinfo>V 1.5: Element deprecated, replaced by OperationalPointStatus</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ocp" type="rcs:tResourceRef">
					<xs:annotation>
						<xs:documentation>Reference to the OCP</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="state">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="current">
								<xs:annotation>
									<xs:documentation>Current state</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:union>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="normal"/>
												<xs:enumeration value="outOfService"/>
												<xs:enumeration value="holdTrain"/>
												<xs:enumeration value="runThru"/>
											</xs:restriction>
										</xs:simpleType>
										<xs:simpleType>
											<xs:restriction base="xs:string"/>
										</xs:simpleType>
									</xs:union>
								</xs:simpleType>
							</xs:element>
							<xs:element name="source" type="rcs:tRequestSource" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Source of current state</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="next" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Requested state</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="rcs:OcpServiceCommand"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="valid" type="rcs:tValidity">
					<xs:annotation>
						<xs:documentation>State validity information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="trains" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="current" type="rcs:tResourceRef" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Reference to a currently present train</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="departure" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Planned time of departure for current train</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="next" type="rcs:tResourceRef" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Reference to the next expected train</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="arrival" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Expected time of arrival for the next train</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="property" type="rcs:tProperty" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Optional named properties</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
